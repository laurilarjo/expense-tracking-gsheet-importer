{
    "compilerOptions": {
        "module": "commonjs",
        "outDir": "./dist",
        "allowJs": true,
        "lib": ["ES2021"],
        "target": "ES2021",
        "allowUnusedLabels": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "sourceMap": true,
        "resolveJsonModule": true,
    },
    "include": [
        "./src/**/*"
    ]
}